{
  "name": "pinch-zoom-browser",
  "version": "0.1.0",
  "description": "Browser library for multi-touch gestures to zoom and drag on any DOM element.",
  "author": "Matth√§us J. N. Beyrle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/magynhard/pinch-zoom-browser.git"
  },
  "keywords": [
    "pinchzoom",
    "pinch",
    "zoom",
    "gesture",
    "touch",
    "drag",
    "browser"
  ],
  "files": [
    "dist/pinch-zoom-browser.js",
    "dist/pinch-zoom-browser.min.js",
    "src/pinch-zoom-browser.js"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "uglify-js": "^3.7.7"
  },
  "scripts": {
    "build": "npm run babelfy && npm run uglify",
    "babelfy": "node node_modules/babel-cli/bin/babel.js \"src/pinch-zoom-browser.min.js\" --out-file \"dist/pinch-zoom-browser.min.js\"",
    "uglify": "node node_modules/uglify-js/bin/uglifyjs \"dist/pinch-zoom-browser.min.js\" -m -c -o \"dist/pinch-zoom-browser.min.js\""
  }
}
